# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
quote_type = single
trim_trailing_whitespace = true

[*.json]
indent_style = space
indent_size = 2

[index.html]
insert_final_newline = false

[*.env*]
insert_final_newline = false
